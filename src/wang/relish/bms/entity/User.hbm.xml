<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="wang.relish.bms.entity.User" table="user" schema="" catalog="bms">
        <id name="id">
            <column name="id" sql-type="int unsigned(10)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="pwd">
            <column name="pwd" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="type">
            <column name="type" sql-type="int(10)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>